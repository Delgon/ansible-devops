---
apiVersion: v1
kind: ConfigMap
metadata:
  name: km-controller
  namespace: mas-{{ mas_instance_id }}-aibroker
  labels:
    app: km-controller
data:
  # Container registry server
  DOCKER_SERVER: docker.io
---
apiVersion: v1
kind: ConfigMap
metadata:
  name: km-controller-logger
  namespace: mas-{{ mas_instance_id }}-aibroker
  labels:
    app: km-controller
data:
  # Modules logging level
  logger.json: |
    {
      "App": {
        "level": "info"
      },
      "Storage": {
        "level": "info"
      }
    }
